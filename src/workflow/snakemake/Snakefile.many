GENES=["SDHB", "MUTYH"]

rule all:
    input:
        expand('{gene}_counts.txt', gene=GENES)

rule gene_sample_counts:
    input:
        "GTEx_Analysis_2017-06-05_v8_RNASeQCv1.1.9_gene_reads.acmg_59.gct.gz"
    output:
        expand('{gene}_counts.txt', gene=GENES)
    shell:
        "for gene in {GENES}; do "
            "python get_gene_counts.py {input} $gene $gene\_counts.txt;"
        "done"
